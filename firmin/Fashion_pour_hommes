<script>
	      jQuery(function ($) {

            $(".nav-link > a").click(function() {
          $(".dropdown").slideUp(200);
          if (
            $(this)
              .parent()
              .hasClass("active")
          ) {
            $(".nav-link").removeClass("active");
            $(this)
              .parent()
              .removeClass("active");
          } else {
            $(".nav-link").removeClass("active");
            $(this)
              .next(".dropdown")
              .slideDown(200);
            $(this)
              .parent()
              .addClass("active");
          }
        });
    </script>
